{
    "name": "protobile/protobile",
    "description": "Protobile is a PHP framework",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Protobile",
            "email": "protobile@googlegroups.com"
        }
    ],
    "require": {
        "codegyre/robo": "~0.4.6",
        "hoa/console": "~2.0"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.0",
        "fabpot/php-cs-fixer": "~1.2",
        "elfet/deployer": "~2.0"
    },
    "autoload": {
        "psr-0": {
            "Protobile\\": ".",
            "App\\": "."
        },
        "files": [ "Protobile/functions.php" ],
        "classmap" : [ "Protobile/", "App/" ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "bin":["proto"]
}